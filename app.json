{
  "expo": {
    "name": "GameDex",
    "slug": "GameDex-Frontend",
    "version": "1.0.0",
    "compileSdkVersion": 35,
    "targetSdkVersion": 35,
    "buildToolsVersion": "52.0.11",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atlas004.GameDexFrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atlas004.GameDexFrontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "scheme": "your-app-scheme",
    "assets": [
      "./assets/fonts/"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "befdd70c-469b-4c5c-8dd1-d27086d996b0"
      }
    }
  }
}


